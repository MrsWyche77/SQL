INSERT INTO inventory VALUES (15, "gift boxes", 150, null,"No");

/*Ordering the items by the price*/

SELECT * FROM inventory ORDER BY "price";

/* What items are 50 and above?*/

SELECT * FROM inventory WHERE "quantity_cases" >=50

